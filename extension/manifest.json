{
	"name" : "You Should See This",
	"description" : "Leave hidden links for each other.",
	"version" : "0.0.1",
	"manifest_version" : 3,
	"background" : {
		"service_worker" : "background.js"
	},
	"icons" : {
		"128" : "/images/eyes_128.png"
	},
	"permissions" : [
		"activeTab",
		"tabs",
		"storage"
	],
	"host_permissions" : [
		"https://ysst.herokuapp.com/*"
	],
	"action" : {
		"default_popup" : "popup.html",
		"default_icon" : {
			"128" : "/images/eyes_128.png"
		}
	}
}